<%= form_for([:admin, user_survey]) do |f| %>
  <% if user_survey.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(user_survey.errors.count, "error") %> prohibited this user_survey from being saved:</h2>

      <ul>
      <% user_survey.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :user_id %>
    <%= f.select :user_id, User.all.map{|u| [u.display_name, u.id]} %>
  </div>

  <div class="field">
    <%= f.label :user_type_id %>
    <%= f.select :user_type_id, UserType.all.map{|u| [u.display_name, u.id]} %>
  </div>

  <div class="field">
    <%= f.label :level_id %>
    <%= f.select :level_id, Level.all.map{|l| [l.display_name, l.id]} %>
  </div>

  <div class="field">
    <%= f.label :group_id %>
    <%= f.select :group_id, Group.all.map{|g| [g.display_name, g.id]} %>
  </div>

  <div class="field">
    <%= f.label :interval_id %>
    <%= f.select :interval_id, Interval.all.map{|i| [i.display_name, i.id]} %>
  </div>

  <div class="field">
    <%= f.label :subject_id %>
    <%= f.select :subject_id, Subject.all.map{|s| [s.display_name, s.id]} %>
  </div>

  <div class="field">
    <%= f.label :activa %>
    <%= f.check_box :activa %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
