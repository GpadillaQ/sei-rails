<p id="notice"><%= notice %></p>

<h1>Evaluaciones de Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Evaluador</th>
      <th>Tipo de Evaluador</th>
      <th>Nivel</th>
      <th>Grupo</th>
      <th>Intervalo</th>
      <th>Asignatura</th>
      <th>Activa?</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_surveys.each do |user_survey| %>
      <tr>
        <td><%= user_survey.user.display_name %></td>
        <td><%= user_survey.user_type.display_name %></td>
        <td><%= user_survey.level.display_name %></td>
        <td><%= user_survey.group.display_name %></td>
        <td><%= user_survey.interval.display_name %></td>
        <td><%= user_survey.subject.display_name %></td>
        <td><%= user_survey.activa %></td>
        <td><%= link_to 'Show', admin_user_survey_path(user_survey) %></td>
        <td><%= link_to 'Edit', edit_admin_user_survey_path(user_survey) %></td>
        <td><%= link_to 'Destroy', admin_user_survey_path(user_survey), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

