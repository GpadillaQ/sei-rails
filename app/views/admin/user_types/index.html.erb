<div class="small-8 column">

  <h1>Tipo de Usuarios</h1>

  <table class="table" border="0" cellpadding="0" cellspacing="0">
    <thead>
      <tr>
        <th>Nombre</th>
      </tr>
    </thead>

    <tbody>
      <% @user_types.each do |user_type| %>
        <tr>
          <td><%= user_type.nombre %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
