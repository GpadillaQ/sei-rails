<p id="notice"><%= notice %></p>

<h1>Resultado de las evaluaciones</h1>

<table>
  <thead>
    <tr>
      <th>Tipo de Usuario</th>
      <th>Grupo</th>
      <th>Nivel</th>
      <th>Intervalo</th>
      <th>Asignatura</th>
      <th>Pregunta</th>
      <th>Respuesta</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @survey_results.each do |survey_result| %>
      <tr>
        <td><%= survey_result.user_survey.user_type.display_name %></td>
        <td><%= survey_result.user_survey.group.display_name %></td>
        <td><%= survey_result.user_survey.level.display_name %></td>
        <td><%= survey_result.user_survey.interval.display_name %></td>
        <td><%= survey_result.user_survey.subject.display_name %></td>
        <td><%= survey_result.question.display_name %></td>
        <td><%= survey_result.answer.display_name %></td>
        <td><%= link_to 'Show', admin_survey_result_path(survey_result) %></td>
        <td><%= link_to 'Edit', edit_admin_survey_result_path(survey_result) %></td>
        <td><%= link_to 'Destroy', admin_survey_result_path(survey_result), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subject Result', new_admin_survey_result_path %>
